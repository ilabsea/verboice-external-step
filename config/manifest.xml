<?xml version="1.0" encoding="UTF-8" ?>
<verboice-service>
  <name>Detect Mobile Operator</name>
  <global-settings>
    <variable name="service_domain" display-name="Service Domain" type="string"/>
  </global-settings>
  <steps>
    <step name="results"
      display-name="Detect Mobile Operator"
      icon="medicalkit"
      type="callback"
      callback-url="http://{service_domain}/detect_operator">
      <response type="variables">
        <variable name="result" display-name="Result" type="string"/>
      </response>
    </step>
  </steps>
</verboice-service>
